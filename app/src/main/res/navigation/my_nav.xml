<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/home">
    <fragment
        android:id="@+id/home"
        android:name="com.example.siingat.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/calendar"
        android:name="com.example.siingat.Calendar"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" >
        <action
            android:id="@+id/action_calendar_to_kalender_detail"
            app:destination="@id/kalender_detail" />
    </fragment>
    <fragment
        android:id="@+id/kalender_detail"
        android:name="com.example.siingat.kalender_detail"
        android:label="fragment_kalender_detail"
        tools:layout="@layout/fragment_kalender_detail" />
</navigation>